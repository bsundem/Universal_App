{
  "app": {
    "debug": false,
    "title": "Universal App",
    "theme": "clam",
    "temp_dir": null,
    "data_dir": "data"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S",
    "file": null,
    "max_size": 10485760,
    "backup_count": 5
  },
  "r": {
    "enabled": true,
    "scripts_dir": "r_scripts",
    "libraries": [
      "base",
      "stats"
    ]
  },
  "kaggle": {
    "enabled": true,
    "credentials_file": "~/.kaggle/kaggle.json",
    "timeout": 60,
    "max_dataset_size_mb": 1000
  }
}